package com.zs.cws.schooladmin.announcements.model;

public class AnnouncementsModel {

	String type;
	String subject;
	String content;
	public AnnouncementsModel() {
		super();
		// TODO Auto-generated constructor stub
	}
	public AnnouncementsModel(String type, String subject, String content) {
		super();
		this.type = type;
		this.subject = subject;
		this.content = content;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	@Override
	public String toString() {
		return "AnnouncementsModel [type=" + type + ", subject=" + subject + ", content=" + content + "]";
	}
	
}
