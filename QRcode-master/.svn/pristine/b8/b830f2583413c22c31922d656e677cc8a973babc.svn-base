package com.zs.cws.utils;

public class MailSentPOJO {
	String from;
	String to;
	String subject;
	String content;
	public MailSentPOJO() {
		super();
		// TODO Auto-generated constructor stub
	}
	public MailSentPOJO(String from, String to, String subject, String content) {
		super();
		this.from = from;
		this.to = to;
		this.subject = subject;
		this.content = content;
	}
	public String getFrom() {
		return from;
	}
	public void setFrom(String from) {
		this.from = from;
	}
	public String getTo() {
		return to;
	}
	public void setTo(String to) {
		this.to = to;
	}
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	
	public String toString() {
		return "MailSentPOJO [from=" + from + ", to=" + to + ", subject=" + subject + ", content=" + content + "]";
	}
	

}
