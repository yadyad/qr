<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<c:set var="req" value="${pageContext.request}" />
<c:set var="baseURL"
	value="${fn:replace(req.requestURL, fn:substring(req.requestURI, 0, fn:length(req.requestURI)), req.contextPath)}" />
<c:url var="myUrl" value="${baseURL}/${MyID}" />
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
<title>SchoolWizard</title>
<link rel="icon" type="image/png" sizes="32x32" href="${myUrl}resources/img/fav.png">
<link
	href="https://fonts.googleapis.com/css?family=Fira+Sans+Condensed:300,400"
	rel="stylesheet">
<link type="text/css" href="${myUrl}resources/css/pop-up-style.css"
	rel="stylesheet">
<link type="text/css" href="${myUrl}resources/css/style.css"
	rel="stylesheet">
<!-- Bootstrap -->
<link type="text/css"
	href="${myUrl}resources/lib/bootstrap-3.3.6-dist/css/bootstrap.min.css"
	rel="stylesheet">
<link type="text/css"
	href="${myUrl}resources/css/font-awesome-4.7.0/css/font-awesome.min.css"
	rel="stylesheet">
	<link type="text/css"
	href="${myUrl}resources/css/font-awesome-4.7.0/css/font-awsom-animate.css"
	rel="stylesheet">
	
	


<!-------------------- data Table--------------------------------------------- -->
<link type="text/css" rel="stylesheet"
	href="${myUrl}resources/lib/Table/css/table-style.css">

<link type="text/css" rel="stylesheet"
	href="${myUrl}resources/lib/datepicker/bootstrap-daterangepicker/daterangepicker.css">

<link type="text/css" rel="stylesheet"
	href="${myUrl}resources/datatablecss/custom.css">
<link type="text/css" rel="stylesheet"
	href="${myUrl}resources/datatablecss/dataTables.bootstrap.min.css">
<link type="text/css" rel="stylesheet"
	href="${myUrl}resources/datatablecss/fixedHeader.bootstrap.min.css">
<link type="text/css" rel="stylesheet"
	href="${myUrl}resources/datatablecss/responsive.bootstrap.min.css">
<link type="text/css" rel="stylesheet"
	href="${myUrl}resources/datatablecss/scroller.bootstrap.min.css">


<!------------------------------------Data Table---------------------------------------- -->

<!--  Angular data table -->
<script src="${myUrl}resources/js/jquery-1.11.3.min.js"></script>
<script src="${myUrl}resources/js/datatable/jquery.dataTables.min.js"></script>
<script src="${myUrl}resources/js/angular/angular.min.js"></script>
<script type="text/javascript"
	src="${myUrl}resources/js/angular/datatable/angular-datatables.js"></script>
<script type="text/javascript"
	src="${myUrl}resources/js/bootstrap.min.js"></script>

<!----------------------------------- datepicker-------------------------------------------- -->
<script type="text/javascript"
	src="${myUrl}resources/lib/datepicker/moment/moment.js"></script>
	<script type="text/javascript"
	src="${myUrl}resources/lib/datepicker/bootstrap-daterangepicker/daterangepicker.js"></script>
